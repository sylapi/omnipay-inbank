{
    "name": "sylapi/omnipay-inbank",
    "description": "InBank gateway for Omnipay payment processing library",
    "type": "library",
    "license": "MIT",
    "require": {
        "league/omnipay": "^3"
    },
    "require-dev": {
      "omnipay/tests": "^4",
        "phpstan/phpstan": "^0.12.85",
        "http-interop/http-factory-guzzle": "^1.2"
    },
    "autoload": {
      "psr-4": {
        "Omnipay\\InBank\\": "src"
      }
    },
    "scripts": {
      "phpstan": "vendor/bin/phpstan",
      "tests": "vendor/bin/phpunit"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
